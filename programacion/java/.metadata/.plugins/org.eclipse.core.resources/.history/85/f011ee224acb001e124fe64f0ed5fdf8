package empresa;

public class programa {

	public static void main(String[] args) throws EmpresaException {
		
		Empleado empleado = new Empleado("Juan", "Perez", "123456789A", 1200d);
		System.out.println(empleado);

		Empresa empresa = new Empresa("Movistar", 123456789, "calle 1");
		System.out.println(empresa);

		Persona persona = new Persona("Juan", "Perez", "123456789A");
		System.out.println(persona);
		
		
	}

}
