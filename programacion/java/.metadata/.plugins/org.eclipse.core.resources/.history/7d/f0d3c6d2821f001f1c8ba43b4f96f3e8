		int max = 128;
		for (int i = 0; i < filas; i++) {
			for (int j = 0; j < columnas; j++) {
				array_type array_element = array[j];
				
			}
		}
		int numAleatorio = (int) (Math.random() * (max - min + 1) + min);
		return numAleatorio;
	}

