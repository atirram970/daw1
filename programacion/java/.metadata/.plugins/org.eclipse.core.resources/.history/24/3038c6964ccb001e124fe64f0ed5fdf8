package empresa;

public class programa {

	public static void main(String[] args) throws EmpresaException {
		try {
			//empleados
			Empleado juan = new Empleado("", "", "", 0d);
			Empleado maria = new Empleado("", "", "", 0d);
			Empleado jose = new Empleado("", "", "", 0d);
			Empleado monica = new Empleado("", "", "", 0d);
			//array empleados
			Empleado[] empleados =  {juan, maria, jose, monica};
			
			Empresa empresa = new Empresa("Zara", "00000000B", "900100200","calle 1", empleados);
			Empleado peper = new Empleado("", "", "", 0d);
			empresa.agregarEmpleado(peper);
			
			Persona persona = new Persona("Juan", "Perez", "123456789A");
		} catch (EmpresaException e) {
			e.printStackTrace();
		} catch (Exception e) {
			e.printStackTrace();
		} catch (Exception e) {}
		
		
		
	}

}
