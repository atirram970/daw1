package RepasoExamenUD6y7.ud6.gestisimal;

public class Articulo {
	//Atributos
	String codigo;
	String descripcion;
	float pc;
	float pv;
	int stock;
	
	//Constructor
	/**
	 * @param codigo
	 * @param descripcion
	 * @param pc
	 * @param pv
	 * @param stock
	 */
	public Articulo(String codigo, String descripcion, float pc, float pv, int stock) {
		super();
		this.codigo = codigo;
		this.descripcion = descripcion;
		this.pc = pc;
		this.pv = pv;
		this.stock = stock;
	}
	
	
}