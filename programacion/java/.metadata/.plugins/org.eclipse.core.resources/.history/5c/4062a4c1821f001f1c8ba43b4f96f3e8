package examenArray;

public class Imagen {

	public static void main(String[] args) {
		generarImagen(5, 5);
	}

	private static int generarImagen(int filas, int columnas) {
		int min = -127;
		int max = 128;
		for (int i = 0; i < filas; i++) {}
		int numAleatorio = (int) (Math.random() * (max - min + 1) + min);
		return numAleatorio;
	}

}
